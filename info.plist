				<key>script</key>
				<string>query=$1
IFS=, read profile bookmark urls &lt;&lt;&lt; "${query}"

browser_urls=
non_browser_urls=

for url in ${urls}
do
	if [[ "${url}" =~ ^http://|^https:// ]]
	then
		browser_urls="${browser_urls} ${url}"
	else
		non_browser_urls="${non_browser_urls} ${url}"
	fi
done

# Open browser urls in chrome
open --new -b com.google.Chrome --args --new-window --profile-directory="${profile}" ${browser_urls}

# Open non-browser urls in default apps
open ${non_browser_urls}</string>
